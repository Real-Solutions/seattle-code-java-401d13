<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".activities.MainActivity">

  <Button
    android:id="@+id/MainActivitySignInBttn"
    android:layout_width="71dp"
    android:layout_height="42dp"
    android:layout_marginStart="16dp"
    android:layout_marginTop="4dp"
    android:layout_marginBottom="557dp"
    android:text="Sign In"
    android:textSize="8sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/MainActivitySignUpBttn"
    android:layout_width="71dp"
    android:layout_height="42dp"
    android:layout_marginTop="4dp"
    android:layout_marginEnd="209dp"
    android:layout_marginBottom="86dp"
    android:text="Sign Up"
    android:textSize="8sp"
    app:layout_constraintBottom_toTopOf="@+id/MainActivityAddASuperPetBttn"
    app:layout_constraintEnd_toStartOf="@+id/MainActivityImageViewUserProfile"
    app:layout_constraintTop_toTopOf="parent" />

  <TextView
    android:id="@+id/MainActivityTextViewGreeting"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="84dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/MainActivityLocationBttn"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="70dp"
    android:text="Locations!"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent" />

  <ImageView
    android:id="@+id/MainActivityImageViewUserProfile"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="4dp"
    android:layout_marginEnd="2dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@android:drawable/btn_star_big_on" />

  <Button
    android:id="@+id/MainActivityAddASuperPetBttn"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="132dp"
    android:text="Add A Super Pet"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.498"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/MainActivitySuperPetBttn"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="44dp"
    android:text="See Super Pets"
    app:layout_constraintBottom_toTopOf="@+id/MainActivityLocationBttn"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
